<div class="container">
  <%= month_calendar(events: @schedules, attribute: :publish_time) do |date, schedules| %>
    <%= date %>
    <% schedules.each do |schedule| %>
      <div class='schedule'>
        <strong><%= truncate(schedule.output.suggested_title.gsub('"',''), length: 24) %></strong>
        <i class="fa-brands fa-tiktok"></i>
      </div>
    <% end %>
  <% end %>
</div>
